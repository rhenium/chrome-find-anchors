{
  "name": "chrome-find-anchors",
  "version": "1.0.0",
  "description": "display hidden anchors",
  "homepage_url": "https://github.com/rhenium/chrome-find-anchors",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Find Anchors"
  },
  "permissions": [
    "activeTab"
  ]
}
